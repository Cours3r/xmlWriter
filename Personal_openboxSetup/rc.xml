		<keybind key="W-C-l">
			<action name="UnmaximiseFull"/>
			<action name="MoveResizeTo">
				<x>-0</x>
				<y>0</y>
				<width>50%</width>
				<height>50%</height>
			</action>
		</keybind>
		<keybind key="W-A-l">
			<action name="UnmaximiseFull"/>
			<action name="MoveResizeTo">
				<x>-0</x>
				<y>-0</y>
				<width>50%</width>
				<height>50%</height>
			</action>
		</keybind>
		<keybind key="W-n">
			<action name="MaximizeFull"/>
		</keybind>
		<keybind key="W-m">
			<action name="UnmaximizeFull"/>
			<action name="MoveResizeTo">
				<width>40%</width>
				<height>40%</height>
			</action>
		</keybind>
		<keybind key="S-A-h">
			<action name="SendToDesktopLeft"/>
		</keybind>
		<keybind key="S-A-l">
			<action name="SendToDesktopRight"/>
		</keybind>
		<keybind key="W-S-Right">
			<action name="DirectionCycleWindows"/>
		</keybind>
		<keybind key="W-S-Left">
			<action name="DirectionCycleWindows"/>
		</keybind>
		<keybind key="W-S-Up">
			<action name="DirectionCycleWindows"/>
		</keybind>
		<keybind key="W-S-Down">
			<action name="DirectionCycleWindows"/>
		</keybind>
		<keybind key="A-Escape">
			<action name="Lower"/>
			<action name="FocusToBottom"/>
			<action name="Unfocus"/>
		</keybind>
		<keybind key="A-space">
			<action name="ShowMenu">
				<menu>client-menu</menu>
			</action>
		</keybind>
		<keybind key="XF86MonBrightnessUp">
			<action name="execute">
				<command>xbacklight +10</command>
			</action>
		</keybind>
		<keybind key="XF86MonBrightnessDown">
			<action name="execute">
				<command>xbacklight -10</command>
			</action>
		</keybind>
		<keybind key="A-F2">
			<action name="execute">
				<command>gmrun</command>
				<smartnotify>
					<enabled>yes</enabled>
					<name>Run Program</name>
				</smartnotify>
			</action>
		</keybind>
		<keybind key="W-S-d">
			<action name="execute">
				<command>dmenu_run -i -nb '#191919' -nf '#fea63c' -sf '#191919' -fn 'NotoMonoRegular:bold:pixelsize=14'</command>
			</action>
		</keybind>
		<keybind key="A-F4">
			<action name="Close"/>
		</keybind>
		<keybind key="A-F5">
			<action name="Iconify"/>
		</keybind>
		<keybind key="A-F6">
			<action name="ToggleDecorations"/>
		</keybind>
		<keybind key="A-F8">
			<action name="ToggleShowDesktop"/>
		</keybind>
		<keybind key="W-d">
			<action name="ToggleShowDesktop"/>
		</keybind>
