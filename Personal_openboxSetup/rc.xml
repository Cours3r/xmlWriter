			<action name="execute">
				<command>openbox -- reconfigure</command>
			</action>
		</keybind>
		<keybind key="A-C-Delete">
			<action name="execute">
				<command>oblogout</command>
			</action>
		</keybind>
		<keybind key="C-A-Right">
