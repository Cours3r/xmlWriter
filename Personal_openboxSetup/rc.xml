		<keybind key="XF86AudioRaiseVolume">
			<action name="execute">
				<command>sh -c "pactl set-sink-mute - false ; pactl set-sink-volume 0 +5%"</command>
			</action>
		</keybind>
		<keybind key="XF86AudioLowerVolume">
			<action name="execute">
				<command>sh -c "pactl set-sink-mute - false ; pactl set-sink-volume 0 -5%"</command>
			</action>
		</keybind>
